{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/index\\.js$",
      "status": 404
    },
    {
      "src": "/dist/index\\.js$",
      "status": 404
    },
    {
      "src": "/dist/index\\.js\\.map$",
      "status": 404
    },
    {
      "src": "/dist/(?!public/).*\\.js$",
      "status": 404
    },
    {
      "src": "/dist/(?!public/).*\\.map$",
      "status": 404
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|map))",
      "dest": "/dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "dist/**"
    }
  }
}

